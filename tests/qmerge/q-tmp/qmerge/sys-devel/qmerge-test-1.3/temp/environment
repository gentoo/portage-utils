p () 
{ 
    einfo "FUNC = $1";
    einfo "EBUILD_PHASE = ${EBUILD_PHASE}";
    [ "$1" = "pkg_${EBUILD_PHASE}" ] || echo "FAIL: EBUILD_PHASE is wrong";
    einfo "ROOT = ${ROOT}";
    einfo "EROOT = ${EROOT}";
    einfo "ED = ${ED}";
    einfo "D = ${D}";
    einfo "T = ${T}";
    [ -d "${T}" ] || echo "FAIL: T does not exist";
    einfo "PN = ${PN}"
}
pkg_nofetch () 
{ 
    __eapi0_pkg_nofetch "$@"
}
pkg_postinst () 
{ 
    p pkg_postinst
}
pkg_postrm () 
{ 
    p pkg_postrm
}
pkg_preinst () 
{ 
    p pkg_preinst;
    [ -d "${D}" ] || echo "FAIL: D does not exist";
    [ -x "${D}/usr/bin/${PN}" ] || echo "FAIL: ${PN} not in /usr/bin"
}
pkg_prerm () 
{ 
    p pkg_prerm
}
pkg_pretend () 
{ 
    p pkg_pretend
}
pkg_setup () 
{ 
    p pkg_setup;
    echo;
    [ "${MERGE_TYPE}" = "binary" ] || echo "FAIL: MERGE_TYPE is wrong";
    echo;
    elog "elog test";
    ewarn "ewarn test";
    eqawarn "eqawarn test";
    eerror "eerror test";
    ebegin "ebegin/eend pass test";
    eend 0 || echo "FAIL: eend did not return 0";
    ebegin "ebegin/eend fail test";
    eend 1 "ignore the !! part" && echo "FAIL: eend did not return 1"
}
src_compile () 
{ 
    __eapi2_src_compile "$@"
}
src_configure () 
{ 
    __eapi2_src_configure "$@"
}
src_install () 
{ 
    printf '#!/bin/sh\necho hi\n' > test.sh;
    newbin test.sh ${PN};
    cat > some.conf  <<-EOF
# file conf
foo
EOF

    insinto /etc;
    doins some.conf;
    newins some.conf another.conf
}
src_test () 
{ 
    __eapi0_src_test "$@"
}
src_unpack () 
{ 
    __eapi0_src_unpack "$@"
}
